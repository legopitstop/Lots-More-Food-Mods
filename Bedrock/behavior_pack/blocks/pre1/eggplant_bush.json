                        }
                    }
                ]
            },
            "event:bone_meal_growth": {
                "trigger": {
                    "event": "event:increase_stage"
                },
                "decrement_stack": {},
                "run_command": {
                    "command": [
                        "particle minecraft:crop_growth_emitter ~ ~ ~",
                        "playsound item.bone_meal.use @p ~ ~ ~"
                    ]
                }
            },
            "event:increase_stage": {
                "condition": "q.block_property('morefood:stage')!=7",
                "set_block_property": {
                    "morefood:stage": "q.block_property('morefood:stage')+1"
                }
            }
