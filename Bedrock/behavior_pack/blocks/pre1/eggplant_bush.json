        "events": {
            "event:on_interact": {
                "sequence": [
                    {
                        "condition": "q.is_item_name_any('slot.weapon.mainhand','minecraft:bone_meal') && q.block_property('morefood:stage') != 7",
                        "trigger": {
                            "event": "event:bone_meal_growth"
                        }
                    },
                    {
                        "condition": "q.block_property('morefood:stage') >= 4 && q.block_property('morefood:stage') <= 6 && !q.is_item_name_any('slot.weapon.mainhand','minecraft:bone_meal')",
                        "spawn_loot": {
                            "table": "loot_tables/morefood/on_interact/eggplant_bush_stage2.json"
                        },
                        "set_block_property": {
                            "morefood:stage": "1"
                        },
                        "play_sound": {
                            "sound": "block.sweet_berry_bush.pick"
                        }
                    },
                    {
                        "condition": "q.block_property('morefood:stage') == 7 && !q.is_item_name_any('slot.weapon.mainhand','minecraft:bone_meal')",
                        "spawn_loot": {
                            "table": "loot_tables/morefood/on_interact/eggplant_bush_stage3.json"
                        },
                        "set_block_property": {
